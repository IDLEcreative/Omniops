{
  "name": "customer-service-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "ls -la lib/supabase/ && echo 'Supabase files found'",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 50",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:unit": "jest --config=config/jest/jest.unit.config.js",
    "test:integration": "jest --config=config/jest/jest.integration.config.js",
    "test:integration:watch": "jest --config=config/jest/jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config=config/jest/jest.integration.config.js --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test:integration",
    "benchmark": "tsx test-agent-performance-benchmark.ts",
    "benchmark:verbose": "tsx test-agent-performance-benchmark.ts --verbose",
    "benchmark:detailed": "tsx test-agent-performance-benchmark.ts --detailed",
    "migrate:encrypt-credentials": "tsx scripts/migrate-credentials.ts",
    "optimize-data": "tsx scripts/optimize-existing-data.ts",
    "deduplicate:embeddings": "node scripts/deduplicate-embeddings.js",
    "deduplicate:cache:clear": "node -e \"const { getDeduplicator } = require('./lib/embedding-deduplicator'); getDeduplicator().clearAllCaches().then(() => process.exit(0));\"",
    "check:deps": "npm ls",
    "check:all": "npm run check:deps && npm run lint && npx tsc --noEmit",
    "worker:start": "node lib/workers/scraper-worker-service.js",
    "worker:dev": "ts-node lib/workers/scraper-worker-service.ts",
    "worker:build": "tsc lib/workers/scraper-worker-service.ts --outDir dist/workers",
    "queue:clean": "node -e \"const { getQueueManager } = require('./lib/queue/scrape-queue'); getQueueManager().cleanup().then(() => process.exit(0));\"",
    "queue:stats": "node -e \"const { getQueueManager } = require('./lib/queue/scrape-queue'); getQueueManager().getQueueMetrics().then(stats => { console.log(JSON.stringify(stats, null, 2)); process.exit(0); });\"",
    "scraper:cleanup": "tsx scripts/scraper-cleanup.ts run",
    "scraper:cleanup:watch": "tsx scripts/scraper-cleanup.ts watch",
    "scraper:status": "tsx scripts/scraper-cleanup.ts status",
    "scraper:crawl": "tsx scripts/run-force-crawl-with-cleanup.ts",
    "embeddings:clean": "tsx clean-contaminated-embeddings.ts",
    "embeddings:test": "tsx test-navigation-fix.ts",
    "embeddings:health": "tsx monitor-embeddings-health.ts",
    "reindex": "tsx scripts/reindex.ts",
    "redis:start": "redis-server docker/redis.conf",
    "redis:cli": "redis-cli",
    "redis:flush": "redis-cli FLUSHALL",
    "docker:up": "docker-compose -f docker/docker-compose.dev.yml up",
    "docker:up:detached": "docker-compose -f docker/docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.dev.yml down",
    "monitor:health": "node -e \"const monitor = require('./lib/monitoring/scrape-monitor'); monitor.checkSystemHealth().then(health => { console.log(JSON.stringify(health, null, 2)); process.exit(0); });\"",
    "seed:dashboard": "bash ./scripts/apply-dashboard-seed.sh",
    "monitor:telemetry": "tsx scripts/monitor-telemetry-rollups.ts",
    "monitor:gdpr": "tsx scripts/monitor-gdpr-audit.ts",
    "backfill:gdpr": "tsx scripts/backfill-gdpr-audit.ts",
    "test:telemetry-smoke": "playwright test __tests__/playwright/telemetry-smoke.spec.ts --project=chromium",
    "test:gdpr-smoke": "playwright test __tests__/playwright/gdpr-privacy.spec.ts --project=chromium",
    "docs:audit": "tsx scripts/audit-doc-versions.ts",
    "docs:audit:report": "tsx scripts/audit-doc-versions.ts --report",
    "docs:check-reviews": "tsx scripts/schedule-doc-reviews.ts --check",
    "audit:brands": "tsx scripts/audit-brand-references.ts",
    "monitor:logs": "bash scripts/monitor-brand-references.sh",
    "debug:setup-rag": "curl \"http://localhost:3000/api/setup-rag?domain=",
    "debug:fix-rag": "curl -X POST \"http://localhost:3000/api/fix-rag\" -H \"Content-Type: application/json\" -d '{\"domain\":\"",
    "debug:test-woo": "curl \"http://localhost:3000/api/test-woocommerce?domain=",
    "debug:rag": "curl \"http://localhost:3000/api/debug-rag?domain=",
    "debug:fix-config": "curl -X POST \"http://localhost:3000/api/fix-customer-config\" -H \"Content-Type: application/json\" -d '{\"domain\":\"",
    "prepare": "node -e \"try { require('husky') } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') throw e }\" || true"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@jest/globals": "^30.2.0",
    "@mozilla/readability": "^0.6.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.1",
    "@types/react-window": "^1.8.8",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.0",
    "bullmq": "^5.61.2",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "crawlee": "^3.15.2",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^4.5.3",
    "glob": "^11.0.3",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.1.3",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.400.0",
    "next": "^15.1.3",
    "next-themes": "^0.4.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.52.1",
    "playwright": "^1.55.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-window": "^2.2.2",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.0",
    "turndown": "^7.1.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.19.23",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/turndown": "^5.0.6",
    "@types/uuid": "^9.0.7",
    "chalk": "^4.1.2",
    "eslint": "^8",
    "eslint-config-next": "^15.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "2.10.5",
    "pg": "^8.16.3",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
