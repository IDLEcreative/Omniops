{
  "name": "customer-service-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:unit": "jest --testMatch='**/__tests__/**/*.test.ts' --testPathIgnorePatterns='**/__tests__/integration/**'",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:integration:watch": "jest --config=jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config=jest.integration.config.js --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test:integration",
    "migrate:encrypt-credentials": "tsx scripts/migrate-credentials.ts",
    "optimize-data": "tsx scripts/optimize-existing-data.ts",
    "check:deps": "npm ls",
    "check:all": "npm run check:deps && npm run lint && npx tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@crawlee/playwright": "^3.13.5",
    "@supabase/supabase-js": "^2.39.3",
