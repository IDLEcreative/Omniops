    "cheerio": "^1.0.0",
    "crypto-js": "^4.2.0",
    "ioredis": "^5.3.0",
    "jsdom": "^24.0.0",
    "js-yaml": "^4.1.0",
    "lz-string": "^1.5.0",
    "next": "15.1.3",
    "openai": "^4.52.1",
    "playwright": "^1.49.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "readability": "^0.4.5",
    "redis": "^4.6.12",
    "tiktoken": "^1.0.10",
