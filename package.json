{
  "name": "customer-service-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:full": "bash scripts/dev-with-tests.sh",
    "prebuild": "ls -la lib/supabase/ && echo 'Supabase files found'",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:widget": "node scripts/build-widget.js",
    "build:widget:analyze": "node scripts/build-widget.js --analyze",
    "build:widget:lazy": "node scripts/build-widget-lazy.js",
    "build:widget:lazy:analyze": "node scripts/build-widget-lazy.js --analyze",
    "build:embed": "node scripts/build-embed.js",
    "build:widgets": "npm run build:widget && npm run build:embed",
    "start": "next start",
    "start:server": "NODE_ENV=production tsx server.ts",
    "lint": "eslint . --max-warnings 50",
    "lint:fix": "eslint . --fix",
    "test": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' jest --bail --passWithNoTests",
    "test:unit": "jest --config=config/jest/jest.unit.config.js",
    "test:integration": "jest --config=config/jest/jest.integration.config.js",
    "test:integration:watch": "jest --config=config/jest/jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config=config/jest/jest.integration.config.js --coverage",
    "test:integration:e2e": "echo '⚠️  Requires dev server running on port 3000 (npm run dev)' && SKIP_E2E=false jest __tests__/integration/agent-flow-metadata-tracking.test.ts __tests__/integration/agent4-correction-tracking.test.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test:integration",
    "benchmark": "tsx test-agent-performance-benchmark.ts",
    "benchmark:verbose": "tsx test-agent-performance-benchmark.ts --verbose",
    "benchmark:detailed": "tsx test-agent-performance-benchmark.ts --detailed",
    "migrate:encrypt-credentials": "tsx scripts/migrate-credentials.ts",
    "optimize-data": "tsx scripts/optimize-existing-data.ts",
    "deduplicate:embeddings": "node scripts/deduplicate-embeddings.js",
    "deduplicate:cache:clear": "node -e \"const { getDeduplicator } = require('./lib/embedding-deduplicator'); getDeduplicator().clearAllCaches().then(() => process.exit(0));\"",
    "check:deps": "npm ls",
    "check:bundle": "node scripts/monitoring/check-bundle-size.js",
    "check:all": "npm run check:deps && npm run lint && NODE_OPTIONS='--max-old-space-size=4096' npx tsc --noEmit",
    "backup:env": "bash scripts/backup-env.sh",
    "validate:env": "bash scripts/validate-env.sh",
    "cleanup:processes": "bash scripts/cleanup-processes.sh",
    "monitor:health": "node -e \"const monitor = require('./lib/monitoring/scrape-monitor'); monitor.checkSystemHealth().then(health => { console.log(JSON.stringify(health, null, 2)); process.exit(0); });\"",
    "predev": "npm run validate:env || true",
    "pretest": "npm run cleanup:processes || true",
    "worker:start": "node lib/workers/scraper-worker-service.js",
    "worker:dev": "ts-node lib/workers/scraper-worker-service.ts",
    "worker:build": "tsc lib/workers/scraper-worker-service.ts --outDir dist/workers",
    "queue:clean": "node -e \"const { getQueueManager } = require('./lib/queue/scrape-queue'); getQueueManager().cleanup().then(() => process.exit(0));\"",
    "queue:stats": "node -e \"const { getQueueManager } = require('./lib/queue/scrape-queue'); getQueueManager().getQueueMetrics().then(stats => { console.log(JSON.stringify(stats, null, 2)); process.exit(0); });\"",
    "scraper:cleanup": "tsx scripts/scraper-cleanup.ts run",
    "scraper:cleanup:watch": "tsx scripts/scraper-cleanup.ts watch",
    "scraper:status": "tsx scripts/scraper-cleanup.ts status",
    "scraper:crawl": "tsx scripts/run-force-crawl-with-cleanup.ts",
    "embeddings:clean": "tsx clean-contaminated-embeddings.ts",
    "embeddings:test": "tsx test-navigation-fix.ts",
    "embeddings:health": "tsx monitor-embeddings-health.ts",
    "reindex": "tsx scripts/reindex.ts",
    "redis:start": "redis-server docker/redis.conf",
    "redis:cli": "redis-cli",
    "redis:flush": "redis-cli FLUSHALL",
    "docker:up": "docker-compose -f docker/docker-compose.dev.yml up",
    "docker:up:detached": "docker-compose -f docker/docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.dev.yml down",
    "seed:dashboard": "bash ./scripts/apply-dashboard-seed.sh",
    "monitor:telemetry": "tsx scripts/monitor-telemetry-rollups.ts",
    "monitor:gdpr": "tsx scripts/monitor-gdpr-audit.ts",
    "backfill:gdpr": "tsx scripts/backfill-gdpr-audit.ts",
    "test:e2e": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:watch-files": "tsx scripts/watch-e2e.ts",
    "test:e2e:critical": "playwright test __tests__/playwright/core-journeys --project=chromium",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:telemetry-smoke": "playwright test __tests__/playwright/telemetry-smoke.spec.ts --project=chromium",
    "test:gdpr-smoke": "playwright test __tests__/playwright/gdpr-privacy.spec.ts --project=chromium",
    "docs:audit": "tsx scripts/audit-doc-versions.ts",
    "docs:audit:report": "tsx scripts/audit-doc-versions.ts --report",
    "docs:check-reviews": "tsx scripts/schedule-doc-reviews.ts --check",
    "agent:extract-workflows": "tsx scripts/extract-workflows-from-e2e.ts",
    "agent:generate-knowledge": "tsx scripts/generate-agent-training-data.ts",
    "agent:regenerate": "npm run agent:extract-workflows && npm run agent:generate-knowledge",
    "agent:setup-hooks": "bash scripts/setup-git-hooks.sh",
    "audit:brands": "tsx scripts/audit-brand-references.ts",
    "monitor:logs": "bash scripts/monitor-brand-references.sh",
    "monitor:tokens": "npx tsx scripts/monitoring/track-token-usage.ts",
    "monitor:alerts": "npx tsx scripts/monitoring/check-token-anomalies.ts",
    "monitor:report": "npx tsx scripts/monitoring/generate-weekly-report.ts --save",
    "monitor:dashboard": "echo 'See scripts/monitoring/dashboard-query.sql for analytics queries'",
    "debug:setup-rag": "curl \"http://localhost:3000/api/setup-rag?domain=",
    "debug:fix-rag": "curl -X POST \"http://localhost:3000/api/fix-rag\" -H \"Content-Type: application/json\" -d '{\"domain\":\"",
    "debug:test-woo": "curl \"http://localhost:3000/api/test-woocommerce?domain=",
    "debug:rag": "curl \"http://localhost:3000/api/debug-rag?domain=",
    "debug:fix-config": "curl -X POST \"http://localhost:3000/api/fix-customer-config\" -H \"Content-Type: application/json\" -d '{\"domain\":\"",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@jest/globals": "^30.2.0",
    "@mozilla/readability": "^0.6.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@react-email/components": "^1.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.3",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.0",
    "bullmq": "^5.61.2",
    "chartjs-node-canvas": "^5.0.0",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "crawlee": "^3.15.2",
    "cron-parser": "^5.4.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^4.5.3",
    "framer-motion": "^12.23.24",
    "glob": "^11.0.3",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.1.3",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.400.0",
    "next": "^15.1.3",
    "next-themes": "^0.4.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "openai": "^4.52.1",
    "papaparse": "^5.5.3",
    "playwright": "^1.55.0",
    "react": "^19.0.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.0.0",
    "react-email": "^5.0.1",
    "recharts": "^2.15.4",
    "resend": "^6.4.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "stripe": "^14.0.0",
    "tailwind-merge": "^2.5.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "turndown": "^7.1.2",
    "typescript": "^5.9.3",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.23",
    "@types/papaparse": "^5.5.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/turndown": "^5.0.6",
    "@types/uuid": "^9.0.7",
    "chalk": "^4.1.2",
    "cross-fetch": "^4.1.0",
    "esbuild": "^0.25.11",
    "eslint": "^8",
    "eslint-config-next": "^15.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "2.10.5",
    "pg": "^8.16.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0"
  }
}
